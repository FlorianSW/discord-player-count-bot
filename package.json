{
  "name": "discord-player-count",
  "version": "1.0.0",
  "exports": "./dist/index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "del-cli dist && tsc",
    "prestart": "npm run build",
    "start": "node ./dist/index.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "@types/gamedig": "^5.0.3",
    "del-cli": "^6.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "jest": {
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [".ts"],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "transformIgnorePatterns": ["node_modules/(?!got)"],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "engines": {
    "node": "20.x.x"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "type": "module",
  "author": "Florian Schmidt",
  "license": "MIT",
  "dependencies": {
    "cftools-sdk": "^3.5.0",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "gamedig": "^5.2.0",
    "node-fetch": "^3.3.2",
    "rxjs": "^7.8.1"
  }
}
